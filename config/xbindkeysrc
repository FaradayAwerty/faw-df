
"$TERMINAL"
	Mod4+Shift+Return

"$RUNMENU"
	Mod4+o


"dmenu_tmux"
	Mod4+t

"dmenu_emoji"
	Mod4+e

"dmenu_unicode"
	Mod4+u

"dmenu_kaomoji"
	Mod4+Shift+u

"dmenu_surf"
	Mod4+b

"dmenu_search"
	Mod4+Shift+b


"volman up"
	XF86AudioRaiseVolume

"volman down"
	XF86AudioLowerVolume

"volman mute"
	XF86AudioMute

"cmus-remote -u"
	Mod4+backslash

"cmus-remote -r"
	Mod4+bracketleft

"cmus-remote -n"
	Mod4+bracketright

"takescreenshot"
	Print

