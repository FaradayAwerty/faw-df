# --- movement --- #
keymap	h	MOVE_LEFT
keymap	j	MOVE_DOWN
keymap	k	MOVE_UP
keymap	l	MOVE_RIGHT

keymap J NEXT_LINK
keymap K PREV_LINK

keymap	b	PREV_WORD
keymap	w	NEXT_WORD

keymap	C-e	UP
keymap	C-y	DOWN

keymap	C-d	UP 12
keymap	C-u	DOWN 12

keymap	C-b	PREV_PAGE
keymap	C-f	NEXT_PAGE

keymap	gg	BEGIN
keymap	G	END
keymap	0	LINE_BEGIN
keymap	$	LINE_END

keymap	/	SEARCH
keymap	?	SEARCH_BACK
keymap	n	SEARCH_NEXT
keymap	N	SEARCH_PREV


# --- browsing --- #
keymap	o	GOTO
keymap	Q	QUIT
keymap	r	RELOAD

keymap H HISTORY
keymap D DOWNLOAD_LIST

keymap z BACK

keymap f LINK_BEGIN
keymap F MOVE_LIST_MENU

keymap	p	PEEK
keymap O OPTIONS

# --- bookmarks --- #
keymap	m	BOOKMARK
keymap	M	ADD_BOOKMARK

keymap s GOTO 'https://duckduckgo.com/html' # search
keymap S GOTO 'https://duckduckgo.com/' # search


# --- tabs --- #
keymap	tt TAB_LINK
keymap	T NEW_TAB
keymap	gt NEXT_TAB
keymap	gT PREV_TAB

keymap	q	CLOSE_TAB

#keymap C-t TAB_MENU


# --- external programs --- #
keymap i SAVE_IMAGE
keymap I PEEK_IMG
keymap x EXTERN_LINK 'exurl'
keymap X EXTERN 'exurl'
keymap u EXTERN_LINK 'tmux split-pane -v -p20 exurl'
keymap U EXTERN 'tmux split-pane -v -p20 exurl'
keymap E EDIT_SCREEN


# --- unbind keys --- #
keymap W NULL
keymap B NULL
keymap : NULL
keymap ";" NULL










