#! /bin/bash
cd config

mkdir -p ~/.config/zathura
mkdir -p ~/.config/sxiv/exec

ln -snf $(realpath zathura/zathurarc) ~/.config/zathura/zathurarc
ln -snf $(realpath sxiv/exec/key-handler) ~/.config/sxiv/exec/key-handler
ln -snf $(realpath xinitrc) ~/.xinitrc
ln -snf $(realpath xresources) ~/.Xresources

